#include <bits/stdc++.h>
using namespace std;
vector<long long> v1, v2;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
    int n;
    long long temp;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cin >> temp;
        v1.push_back(temp); // 원본
        v2.push_back(temp); // 정렬
    }
    sort(v2.begin(), v2.end());
    v2.erase(unique(v2.begin(), v2.end()), v2.end()); // 정렬된 벡터에서 중복값 제거
    for (int i = 0; i < n; i++)
        cout << lower_bound(v2.begin(), v2.end(), v1[i]) - v2.begin() << " ";
    return 0;
}
/*
lower_bound(시작값, 종료값, 찾는값) : 찾는값보다 같거나 큰값을 리턴, 리턴값이 iterator이므로 begin()을 빼주면 인덱스 구할 수 있음
*/